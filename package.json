  "author": "CrossedMinds <info@crossedminds.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "next start -p ${PORT:=3000}",
    "test": "jest --coverage",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "generate-abis-types": "typechain --target=ethers-v5 'src/Contracts/Abis/*.json'",
    "postinstall": "yarn generate-abis-types"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "pages/**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "server/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
